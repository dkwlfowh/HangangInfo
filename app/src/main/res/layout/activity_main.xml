<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_background"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="6"></LinearLayout>

    <ImageView
        android:id="@+id/toolbar_title"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="9.5"
        android:gravity="center"
        android:src="@drawable/main" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10">

    </LinearLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="5"
        android:src="@drawable/ment"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="8">

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="132"
        android:gravity="center"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="6"></LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="86"
            android:orientation="vertical">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow
                    android:layout_weight="1"
                    android:gravity="center">


                    <ImageButton
                        android:id="@+id/Gwangnaru"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:background="@drawable/gwangnaru"
                        android:src="@drawable/circle_border" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="7.5" />

                    <ImageButton
                        android:id="@+id/Jamsil"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:background="@drawable/jamsil"
                        android:src="@drawable/circle_border" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="7.5" />


                    <ImageButton
                        android:id="@+id/Ttukseom"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:background="@drawable/ttukseom"
                        android:src="@drawable/circle_border" />
                </TableRow>


                <TableRow
                    android:layout_weight="1"
                    android:gravity="center">

                    <ImageButton
                        android:id="@+id/Jamwon"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:background="@drawable/jamwon"
                        android:src="@drawable/circle_border" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="7.5" />

                    <ImageButton
                        android:id="@+id/Banpo"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:background="@drawable/banpo"
                        android:src="@drawable/circle_border" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="7.5" />


                    <ImageButton
                        android:id="@+id/Ichon"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:background="@drawable/ichon"
                        android:src="@drawable/circle_border" />
                </TableRow>


                <TableRow
                    android:layout_weight="1"
                    android:gravity="center">

                    <ImageButton
                        android:id="@+id/Yeouido"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:background="@drawable/yeouido"
                        android:src="@drawable/circle_border" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="7.5" />


                    <ImageButton
                        android:id="@+id/Mangwon"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:background="@drawable/mangwon"
                        android:src="@drawable/circle_border" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="7.5" />


                    <ImageButton
                        android:id="@+id/Nanji"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:background="@drawable/nanji"
                        android:src="@drawable/circle_border" />
                </TableRow>


                <TableRow
                    android:layout_weight="1"
                    android:gravity="center">

                    <ImageButton
                        android:id="@+id/Gangseo"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:background="@drawable/gangseo"
                        android:src="@drawable/circle_border" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="7.5" />


                    <ImageButton
                        android:id="@+id/YangHwa"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:background="@drawable/yanghwa"
                        android:src="@drawable/circle_border" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="7.5" />


                    <ImageButton
                        android:id="@+id/Info"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:background="@drawable/info"
                        android:src="@drawable/circle_border" />
                </TableRow>

            </TableLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="6"></LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="22.167"></LinearLayout>
</LinearLayout>